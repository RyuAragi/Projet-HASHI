<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane styleClass="background" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.example.demojeumenu.MenuPrincipalController">
    <fx:include source="Logo.fxml" />
    <fx:include source="ZoneMenu.fxml" />
    <VBox alignment="CENTER">
        <HBox alignment="CENTER">
            <TextField fx:id="zoneTexte" styleClass="custom-button" />
        </HBox>
        <Button fx:id="bouton" onAction="#afficherAutreMenu" styleClass="custom-button" text="JOUER" />
        <Button onAction="#quitter" styleClass="custom-button" text="QUITTER" />
    </VBox>
</StackPane>
