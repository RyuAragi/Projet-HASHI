<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane styleClass="background" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demojeumenu.MenuPrincipalController">
    <fx:include source="Logo.fxml" />

    <VBox alignment="BASELINE_CENTER">
        <Button styleClass="custom-button-titre" text="Jacobhashi">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
    </VBox>
    <VBox alignment="CENTER">
        <HBox alignment="CENTER">
            <TextField fx:id="zoneTexte" styleClass="custom-button" />
        </HBox>
        <Button fx:id="bouton" onAction="#afficherAutreMenu" styleClass="custom-button" text="JOUER" />
        <Button onAction="#quitter" styleClass="custom-button" text="QUITTER" />
    </VBox>
</StackPane>
