    <StackPane styleClass="StackPane" minWidth="750" minHeight="750">
        <VBox spacing="10" alignment="CENTER">
            <Button fx:id="bouton" styleClass="custom-button" text="Button 1" onAction="#onButton1Click" prefWidth="500" prefHeight="100"/>
