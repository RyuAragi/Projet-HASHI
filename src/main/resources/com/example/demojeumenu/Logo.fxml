<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <ImageView id="logoImageView" fx:id="logoImageView" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER_LEFT">
            <image>
                <Image url="@images/logo.png" />
            </image>
            <BorderPane.margin>
                <Insets left="35.0" top="35.0" />
            </BorderPane.margin>
        </ImageView>
    </top>
</BorderPane>
